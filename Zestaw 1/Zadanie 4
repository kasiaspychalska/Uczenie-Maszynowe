import pandas as pd
questions = pd.read_csv("survey_results_schema.csv", delimiter=',', header=0)
df = pd.read_csv("survey_results_public.csv", delimiter=',', header=0)
df = df.dropna()
# Change value in column WorkWeekHrs to get a Full Time Equivalent (FTE)
df['WorkWeekHrs'] = df['WorkWeekHrs']/40
# Calculation a salary for FTE
df['CompFTE'] = round(df['CompTotal']/df['WorkWeekHrs'], 0)
df = df[['Age', 'CompFTE']]
